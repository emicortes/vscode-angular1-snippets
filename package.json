{
  "name": "Angular1",
  "publisher": "johnpapa",
  "displayName": "Angular 1 JavaScript and TypeScript Snippets",
  "description": "Angular 1 JavaScript and TypeScript snippets",
  "icon": "images/angular-shield.png",
  "galleryBanner": {
    "color": "#0273D4",
    "theme": "dark"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
